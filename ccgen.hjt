\intbl{\ql\li0\fi0\ri0\sb0\sl0\sa0 \plain\f6\fs20\cf3\b NAW Br\cell}
\intbl{\ql\li0\fi0\ri0\sb0\sl0\sa0 \plain\f6\fs20\cf3\b values\cell}
\intbl{\ql\li0\fi0\ri0\sb0\sl0\sa0 \plain\f6\fs20\cf3\b black\cell}
